    <script>

        function highlight()
        {
            // remove the 'highlight' class from any siblings
            var highlighted = this.parentNode.querySelectorAll('.highlight');
            
            for (var i = 0; i < highlighted.length; i++) {
            highlighted[i].classList.remove('highlight');
            }
            
            
            // toggle the 'highlight' class on the clicked item
            this.classList.add('highlight');
        }

        // get all list items
        var items = document.querySelectorAll('li');

        // set the highlight() function as the click handler for each item
        for (var i = 0; i < items.length; i++) {
            items[i].onclick = highlight;
        }

    </script>
